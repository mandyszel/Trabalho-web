<%-include('cabecalho.ejs')%>

<form method="post">
    <input type="text" name="pesquisar" id="pesquisar">
    <input type="submit" value="Pesquisar">
</form>


<table>
    <tr>
        <th>Identificador</th>
        <th>Nome</th>
        <th>Cor</th>
        <th>Ano</th>
        <th>Integrantes</th>
        
    </tr>

    <%equipe.forEach(function(equipe){%>
    <tr>
        <td><%=equipe.equipeidentificador%></td>
        <td><%=equipe.nome%></td>
        <td><%=equipe.cor%></td>
        <td><%=equipe.ano%></td>
        <td><%=equipe.integrantes%></td>
        <td><a href="/edtequipe/<%=equipe._id%>">Editar</a></td>
        <td><a href="/delequipe/<%=equipe._id%>">Delete</a></td>
    </tr>
    <% }) %>
</table>
<%-include('rodape.ejs')%>